var covanentURL = 'http://us.battle.net/api/wow/guild/the-venture-co/the%20grim%20covenant?fields=members';


var remoteListView = require('RemoteListView');

var remoteError = function(e) {
    Ti.API.debug("Status: " + this.status);
    Ti.API.debug("Text: " + this.responseText);
    Ti.API.debug("Error: " + e.error);
    alert("There's a problem pulling remote data");
};


var xhr = Ti.Network.createHTTPClient({
    onload: remoteListView.remote,
    onerror: remoteError,
    timeout:5000
});


//Main Code
xhr.open('GET', covanentURL);
xhr.send();
