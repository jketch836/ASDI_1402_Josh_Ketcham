//Dot Notations Rurouni Kenshin Manga		
	var protagonist = new object();
		protagonist.name = 'Kenshin';
		protagonist.aka = 'Hitokiri Battosai';
		protagonist.robe = 'Red';
		protagonist.age = 28;
		
	var pSword = new object();
		pSword.name = 'Reverse Blade';
		pSword.hilt = ['grip', 'dark grey', '10cm'];
		pSword.blade = '60cm';
		pSword.color = 'grey';
		pSword.weight = '10LB';
		pSword.info = function(){
			return 'This sword is a ' + pSword.name + ' sword. It is a fictional sword.';
		};
		pSword.about = function (){
			return protagonist.name + "'s" + " sword is a " + pSword.name + ". The hilt is"  + pSword.hilt[0] + ", and is " + pSword.hilt[1] + " long. The blade itself is " + pSword.blade + ".";
		};	
	
	var loveIntrest = newobject();
		loveIntrest.name = 'Karou';
		loveIntrest.aka = 'Kenjutsu Princess';
		loveIntrest.robe = 'Light Blue';
		loveIntrest.age = 17;
		
		
	var liSword = newobject();
		liSword.name = 'Bokken';
		liSword.hilt = ['brown', '8cm'];
		liSword.blade = null;
		liSword.weight = '5LB';
		
	
	var antagonist = new object ();
		antagonist.name = 'Shishio';
		antagonist.aka = 'Hitokiri';
		antagonist.robe = 'Purple';
		antagonist.age = 30;
	
		
	var aSword = new object();
		aSword.name = 'Wakizashi';
		aSword.hilt = ['black', '8cm'];
		aSword.blade = '55cm';
		aSword.weight = '8LB';
		aSword.info = function(){
			return 'This sword is a ' + aSword.name + '. It is usually defined by the blade which is usually ' + aSword.blade + '.';
		};
		aSword.about = function (){
			return antagonist.name + "'s" + " sword is a " + aSword.name + ". The hilt is"  + aSword.hilt[0] + ", and is " + aSword.hilt[1] + " long. The blade itself is " + aSword.blade + ".";
		};
	
var mangaInfo = [protagonist, pSword, antagonist, aSword];
// Character information gotten from http://kenshin.wikia.com/
// Sword information gotten from http://books.google.com/books?id=zPyswmGDBFkC&pg=PA48&dq=wakizashi&hl=en&ei=KZYsTo-EIqTd0QGXndDkDg&sa=X&oi=book_result&ct=result&resnum=5&ved=0CDkQ6AEwBA#v=onepage&q=wakizashi&f=true


