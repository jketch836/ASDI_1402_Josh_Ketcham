var memSave = require('favorites');

var toonWin = Ti.UI.createWindow({
	title : 'Council of Judgement Members',
	backgroundColor : '#fff'
});

var toonImage = Ti.UI.createImageView({
	height : '30dp',
	width : '30dp',
	image : 'toon.png'
});
var toonLink = Ti.UI.createView({
	bottom : '0dp',
	width : '175dp',
	height : '60dp',
	left : '0dp',
	backgroundColor : '#fff',
	image : toonImage,
	url : 'remoteAPI.js'
});

var toonText = Ti.UI.createLabel({
	bottom : '5dp',
	text : 'Council Members',
	font : {
		fontStyle : 'Helvetica',
		fontSize : '10dp'
	},
	color : '#000'
});



var favImage = Ti.UI.createImageView({
	height : '30dp',
	width : '30dp',
	image : 'fav.png'
});
var favLink = Ti.UI.createView({
	bottom : '0dp',
	width : '175dp',
	height : '60dp',
	left : '0dp',
	backgroundColor : '#fff',
	image : faveImage,
	url : 'favorites.js'
});

var favText = Ti.UI.createLabel({
	bottom : '5dp',
	text : 'Favorites',
	font : {
		fontStyle : 'Helvetica',
		fontSize : '10dp'
	},
	color : '#000'
});


//Main Code
toonLink.add(toonText);
toonWin.add(toonLink);
toonWin.open();
