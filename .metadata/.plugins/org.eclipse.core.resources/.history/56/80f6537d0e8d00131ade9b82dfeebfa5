//Dot Notations Rurouni Kenshin Manga
var manga = new object();
	manga.author = 'Nobuhiro Watsuki';
	manga.publisher = 'Shueisha';
	manga.magazine = 'Weekly Shonen Jump'; 
	manga.name = 'Rurouni Kenshin';
	manga.volumes = 28;
	manga.chapters = 255;

var storyBackground = new object();
	storyBackground.era = 'Meiji Era';
	storyBackground.year = 1878;
	storyBackground.country = 'Japan';
	storyBackground.city = 'Tokyo';
	
var protagonist = new object();
	protagonist.name = 'Kenshin';
	protagonist.nickname = 'Hitokiri Battosai';
	protagonist.hairColor = 'Red';
	protagonist.phase = 'Oro';
	protagonist.robe = 'Blue';
	protagonist.skill = 85;
	protagonist.age = 28;
	protagonist.friends ['Karou','Sanosuke', 'Yahiko', 'Megumi'];
	
var pSword = new object();
	pSword.name = 'Reverse Blade';
	pSword.hilt = ['grip', 'dark grey', '10cm'];
	pSword.blade = '60cm';
	pSword.color = 'grey';
	pSword.weight = '2LB';
	pSword.cutthrough = false;
	pSword.about = function (){
		console.log(protagonist.name + "'s" + " sword is a " + pSword.name + ". The hilt is"  + pSword.hilt[0] + ", and is " + pSword.hilt[1] + " long. The blade itself is " + pSword.blade + ". It weights " + pSword.weight + ', and it is ' + pSword.cutthrough + ' that it can cut.');
	};	

var antagonist = new object ();
	antagonist.name = 'Shishio';
	antagonist.burned = true;
	antagonist.robe = 'Purple';
	antagonist.skill = 80;
	antagonist.age = 30;
	antagonist.bandages = true;
	
var aSword = new object();
	aSword.name = 'Wakizashi';
	aSword.hilt = ['dark grey', '8cm'];
	aSword.blade = '55cm';
	aSword.weight = '1.5LB';
	aSword.cutthrough = true;
	aSword.info = function(){
		return 'This sword is a ' + aSword.name + '. It is usually defined by the blade which is usually ' + aSword.blade + '.';
	};
	aSword.about = function (){
		return antagonist.name + "'s" + " sword is a " + aSword.name + ". The hilt is"  + aSword.hilt[0] + ", and is " + aSword.hilt[1] + " long. The blade itself is " + aSword.blade + ". It weights " + aSword.weight + ', and it is ' + aSword.cutthrough + ' that it can cut deeply.';
	};
// Character information gotten from http://kenshin.wikia.com/
// Sword information gotten from http://books.google.com/books?id=zPyswmGDBFkC&pg=PA48&dq=wakizashi&hl=en&ei=KZYsTo-EIqTd0QGXndDkDg&sa=X&oi=book_result&ct=result&resnum=5&ved=0CDkQ6AEwBA#v=onepage&q=wakizashi&f=true


