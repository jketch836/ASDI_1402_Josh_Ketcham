var lycanURL = 'http://us.battle.net/api/wow/character/the-venture-co/Lycanthrop';
var havenURL = 'http://us.battle.net/api/wow/character/the-venture-co/Haven';
var vennURL = 'http://us.battle.net/api/wow/character/the-venture-co/Vennigi';


var remoteResponse = function() {
    var remote = json.parse(JSON.responseText);
};

var remoteError = function(e) {
    Ti.API.debug("Status: " + this.status);
    Ti.API.debug("Text: " + this.responseText);
    Ti.API.debug("Error: " + e.error);
    alert("There's a problem pulling remote data");
};

var xhr = Ti.Network.createHTTPClient({
    onload: remoteResponse,
    onerror: remoteError,
    timeout:5000
});

remoteResponse();
xhr.open('GET', lycanURL);
xhr.send();
